<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".IDPWActivity">

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="한글만 입력해주세요."
        android:textColor="#FF0000"
        android:visibility="invisible"
        tools:layout_editor_absoluteX="50dp"
        tools:layout_editor_absoluteY="232dp" />

    <RadioButton
        android:id="@+id/radioButton9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="단체 계정"
        tools:layout_editor_absoluteX="210dp"
        tools:layout_editor_absoluteY="78dp" />

    <RadioButton
        android:id="@+id/radioButton10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="개인 계정"
        tools:layout_editor_absoluteX="123dp"
        tools:layout_editor_absoluteY="78dp" />

    <Button
        android:id="@+id/button5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="확인"
        app:backgroundTint="#3281F8"
        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="396dp" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="이름"
        android:textAllCaps="false"
        android:textColor="#000000"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="50dp"
        tools:layout_editor_absoluteY="145dp" />

    <EditText
        android:id="@+id/editTextTextPersonName14"
        android:layout_width="90dp"
        android:layout_height="50dp"
        android:background="#D9D9D9"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="일"
        android:textColor="#979797"
        tools:layout_editor_absoluteX="280dp"
        tools:layout_editor_absoluteY="305dp" />

    <EditText
        android:id="@+id/editTextTextPersonName15"
        android:layout_width="90dp"
        android:layout_height="50dp"
        android:background="#D9D9D9"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="월"
        android:textColor="#979797"
        tools:layout_editor_absoluteX="174dp"
        tools:layout_editor_absoluteY="305dp" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="생년월일 (YYYY/MM/DD)"
        android:textAllCaps="false"
        android:textColor="#000000"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="50dp"
        tools:layout_editor_absoluteY="262dp" />

    <EditText
        android:id="@+id/editTextTextPersonName16"
        android:layout_width="110dp"
        android:layout_height="50dp"
        android:background="#D9D9D9"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="년도"
        android:textColor="#979797"
        tools:layout_editor_absoluteX="50dp"
        tools:layout_editor_absoluteY="305dp" />

    <EditText
        android:id="@+id/editTextTextPersonName17"
        android:layout_width="320dp"
        android:layout_height="40dp"
        android:background="#D9D9D9"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="이름을 입력해주세요"
        android:textColor="#979797"
        tools:layout_editor_absoluteX="50dp"
        tools:layout_editor_absoluteY="192dp" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="아이디 찾기"
        tools:layout_editor_absoluteX="113dp"
        tools:layout_editor_absoluteY="29dp" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="비밀번호 찾기"
        tools:layout_editor_absoluteX="236dp"
        tools:layout_editor_absoluteY="29dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="34dp"
        android:layout_height="42dp"
        app:srcCompat="?android:attr/textCheckMarkInverse"
        tools:layout_editor_absoluteX="202dp"
        tools:layout_editor_absoluteY="23dp" />

</androidx.constraintlayout.widget.ConstraintLayout>